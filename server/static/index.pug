<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>

    style.
      body {
        padding: 0;
        box-sizing: border-box;
        font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue',sans-serif; 
        width: 835px; 
        margin: auto; 
      }
      .contenedor{
        margin: 5px auto;        
      }
      th, td {
        font-size: 10.5px;
        text-align: center;
      }
      .spaceRows {
        height: 5px;
      }
      tbody > tr > td{
        border: 1px solid #000;
     
      }
      .description {
        word-wrap: break-word;
        width: 120px !important;
      }
      .responsable {
        word-wrap: break-word;
        width: 80px !important;
      }
      .factura {
        word-wrap: break-word;
        width: 20px !important;
      }
  </head>
  <body>
    table.contenedor
      thead
        tr
          th C&oacute;digo
          th Descripci&oacute;n
          th C&eacute;dula
          th Responsable
          th Cond. Ingreso
          th Fecha Ingreso
          th Actualmente
          th Marca
          th Modelo
          th Serial
          th Factura
      tbody
        each value in allData
            tr
              td #{value.codigo}
              td.description #{value.descripcion}
              td #{value.cedula || '*****'}
              td.responsable #{value.responsable || '*****'}
              td #{value.condicion_ingreso}
              td #{value.fecha_ingreso}
              td #{value.estado_actual}
              td #{value.marca}
              td #{value.modelo}
              td #{value.serial}
              td.factura #{value.factura}
            tr.spaceRows
  </body>
</html>
